                    $opt = $item->options;
                    //echo "test";
                    //echo count($opt);
                    for ($c = 0; $c < count($opt); $c++) {
                        //print_r($opt[$c]);
                        foreach ($opt[$c] as $ckey => $cvalue) {
                            $opts[$ckey] = $cvalue;
                        }
                    }
                    //print_r($opts);
                    //exit;
                }

                $optionch = array();
                if (isset($opts)) {
                    $optionch = (array) $opts;
                }

                //echo "<pre>"; print_r($item); die('ccc');


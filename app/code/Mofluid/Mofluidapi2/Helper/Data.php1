                } catch (Exception $ex) {

                }
                $manage_stock = $product_stock['manage_stock'];
                $is_in_stock = $product_stock['is_in_stock'];
                $res["qty_flag"] = $flag;

                if ($item->quantity > $product_stock_quantity) {
                    $res["status"] = "error";
                    $res["type"] = "quantity";
